<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lyk.crm.workbench.dao.ClueRemarkDao">

	<delete id="delete">
		delete from tbl_clue_remark where id=#{id}
	</delete>

	<select id="getRemarkListByClueId" resultType="clue">
		select * from tbl_clue_remark where id=#{clueId}
	</select>
  	
</mapper>

































